            <lb/><expan>ledit</expan> appelle <choice><orig>enuers</orig><reg>envers</reg></choice><subst><del><expan>lesdits</expan></del><add place="superposition"><expan>ledit</expan></add></subst> appellan<subst><del>s</del><add place="superposition">t</add></subst> aux
